<UserControl x:Class="DMT.Controls.Plaza.Logo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DMT.Controls.Plaza"
             mc:Ignorable="d" 
             d:DesignHeight="100"
             d:DesignWidth="150">
    <Grid Margin="1">
        <Border BorderBrush="Silver"
                BorderThickness="1"
                CornerRadius="3"
                Margin="1">
            <Image Source="/DMT.Controls;component/Images/Logo/tollway.png" />
        </Border>
    </Grid>
</UserControl>
